{
  "name": "vue2-webpack-pack",
  "version": "1.0.1",
  "description": "",
  "main": "src/main.js",
  "private": false,
  "scripts": {
    "dev": "npx webpack serve --progress=profile --node-env development --config build/webpack.dev.js",
    "clean": "rimraf dist",
    "package": "npx webpack build --progress=profile --node-env production --config build/webpack.prod.js",
    "build": "npm run clean && npm run package"
  },
  "keywords": [
    "Webpack5",
    "Vue2",
    "pnpm"
  ],
  "author": "cole.yu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.7.15",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "core-js": "^3.33.3",
    "vue-router": "3.6.5"
  },
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.11.0"
  },
  "sideEffects": [
    "dist/*",
    "*.css",
    "*.less",
    "*.vue"
  ],
  "config": {
    "host": "0.0.0.0",
    "port": 8080
  }
}
